{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "tabWidth": 2,
    "useTabs": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "printWidth": 200,
    "trailingComma": "none"
}
